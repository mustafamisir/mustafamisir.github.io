@inproceedings{Xie2014AGenetic,
	author = {J. Xie and  Y. Mei and  A. Ernst and  X. Li and  A. Song},
	year = {2014},
	title = {A Genetic Programming-based Hyper-heuristic Approach for Storage Location Assignment Problem},
	address = {Beijing, China},
	booktitle = {the IEEE Congress of Evolutionary Computation (CEC14)},
  url={http://ieeexplore.ieee.org/abstract/document/6900604/},
  note={This study proposes a method for solving real-world warehouse Storage Location Assignment Problem (SLAP) under grouping constraints by Genetic Programming (GP). Integer Linear Programming (ILP) formulation is used to define the problem. By the proposed GP method, a subset of the items is repeatedly selected and placed into the available current best location of the shelves in the warehouse, until all the items have been assigned with locations. A heuristic matching function is evolved by GP to guide the selection of the subsets of items. Our comparison between the proposed GP approach and the traditional ILP approach shows that GP can obtain near-optimal solutions on the training data within a short period of time. Moreover, the evolved heuristics can achieve good optimization results on unseen scenarios, comparable to that on the scenario used for training. This shows that the evolved heuristics have good reusability and can be directly applied for slightly different scenarios without any new search process.},
}





@inproceedings{Yarimcam2014Heuristic,
	author = {Ahmet Yarimcam and  Shahriar Asta and  Ender Ozcan and  Andrew J. Parkes},
	year = {2014},
	title = {Heuristic Generation via Parameter Tuning for Online Bin Packing},
	address = {Orlando/Florida, USA},
	booktitle = {the IEEE Symposium Series on Computational Intelligence (SSCI)},
	 url = {http://www.cs.nott.ac.uk/~exo/docs/publications/tuningPolicies.pdf},
	 note={Online bin packing requires immediate decisions to be made for placing an incoming item one at a time into bins of fixed capacity without causing any overflow. The goal is to maximise the average bin fullness after placement of a long stream of items. A recent work describes an approach for solving this problem based on a 'policy matrix' representation in which each decision option is independently given a value and the highest value option is selected. A policy matrix can also be viewed as a heuristic with many parameters and then the search for a good policy matrix can be treated as a parameter tuning process. In this study, we show that the Irace parameter tuning algorithm produces heuristics which outperform the standard human designed heuristics for various instances of the online bin packing problem.}
}



